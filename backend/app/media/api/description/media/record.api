syntax = "v1"

info(
    title: "media"
    desc: "media"
    author: "xuesong huang"
)

type Record {
    SubUserId       string  `json:"subUserId"`
    Timestamp       int64   `json:"timestamp"`
    Composition     string  `json:"composition"`
    Reference       string  `json:"reference"`
    Image           string  `json:"image"`
    Video           string  `json:"video"`
    Audio           string  `json:"audio"`
    Midi            string  `json:"midi"`
    Sheet           string  `json:"sheet"`
    Diff            string  `json:"diff"`
    Waterfall       string  `json:"waterfall"`
    Report          string  `json:"report"`
    IsRef           bool    `json:"isRef"`
}

type (
    CreateRecordReq {
        ProfileName     string  `json:"profileName"`
        Reference       string  `json:"reference"`
    }

    CreateRecordResp {
        Code        int64    `json:"code"`
        RecordId    int64    `json:"recordId"`
        Msg         string   `json:"msg"`
    }
)

type (
    DeleteRecordReq {
        ProfileName string  `json:"profileName"`
        RecordId    int64   `json:"recordId"`
    }
    DeleteRecordResp {
        Code    int64      `json:"code"`
        Msg     string     `json:"msg"`
    }
)

type (
    RecordInfo {
        RecordId        int64   `json:"RecordId"`
        Composition     string  `json:"composition"`
        Reference       string  `json:"reference"`
    }
    GetRecordReq {
        ProfileName     string  `json:"profileName"`
        Limit           int64   `json:"limit,optional"`
        Offset          int64   `json:"offset,optional"`
        Start           int64   `json:"start,optional"`
        End             int64   `json:"end,optional"`
    }
    GetRecordResp {
        Code    int64           `json:"code"`
        Data    []RecordInfo    `json:"data"`
        Msg     string          `json:"msg"`
    }
)

type (
    SetAsReferenceReq {
        ProfileName string  `json:"profileName"`
        RecordId    int64   `json:"recordId"`
        IsRef       bool    `json:"isRef"`
    }
    SetAsReferenceResp {
        Code    int64      `json:"code"`
        Msg     string     `json:"msg"`
    }
)

type FileDetails {
    PresignedURL  string    `json:"presignedurl"`
    FileName      string    `json:"fileName"`
}

type (
    GetVideoUrlReq {
        ProfileName string   `json:"profileName"`
        RecordId    int64    `json:"recordId"`
    }

    GetVideoUrlResp {
        Code    int64       `json:"code"`
        Data    FileDetails `json:"data"`
        Msg     string      `json:"msg"`
    }
)

type (
    UploadVideoSuccessReq {
